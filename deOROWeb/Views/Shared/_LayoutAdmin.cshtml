            type: "GET",
            url: "@Request.ApplicationPath/WebUser/GetProfile/?userName=" + '@User.Identity.Name',
            cache: false,
            contentType: "application/json; charset=utf-8",
            success: function (result) {
                $('#popup-modal-profile').html(result);
            },
            error: function (error) {
                //alert("There was an error posting the data to the server: " + error.responseText);
            }
        });
    })


    function ToJavaScriptDate(value) {

        if (value == null)
            return value;

        var dt = new Date(parseInt(value.substr(6)));
        var localDT = convertUTCDateToLocalDate(dt);
        return (localDT.getMonth() + 1) + "/" + localDT.getDate() + "/" + localDT.getFullYear();
    }


    function ToJavaScriptDateTime(value) {

        if (value == null)
            return value;

        var dt = new Date(parseInt(value.substr(6)));
        var localDT = convertUTCDateToLocalDate(dt);
        return (localDT.getMonth() + 1) + "/" + localDT.getDate() + "/" + localDT.getFullYear() + " " + localDT.getHours() + ":" + localDT.getMinutes() + ":" + localDT.getSeconds();
    }

    function convertUTCDateToLocalDate(date) {
        var newDate = new Date(date.getTime() + date.getTimezoneOffset() * 60 * 1000);

        var offset = date.getTimezoneOffset() / 60;
        var hours = date.getHours();

        if (hours - offset > 0) {
            newDate.setHours(hours - offset);
            return newDate;
        }
        else
            return date;
    }

    function isNumeric(n) {
        return !isNaN(parseFloat(n)) && isFinite(n);
    }

    function isInt(n) {
        return Number(n) === n && n % 1 === 0;
    }

    function isFloat(n) {
        return n === Number(n) && n % 1 !== 0;
    }

    $.fn.serializeObject = function () {
        var o = {};
        var a = this.serializeArray();
        $.each(a, function () {
            if (o[this.name] !== undefined) {
                if (!o[this.name].push) {
                    o[this.name] = [o[this.name]];
                }
                o[this.name].push(this.value || '');
            } else {
                o[this.name] = this.value || '';
            }
        });
        return o;
    };
</script>
